{
    "name": "isolate/unit-of-work",
    "description": "Unit of Work",
    "keywords": ["isolate", "unit of work", "uow", "unit", "work"],
    "require": {
        "php": ">=5.4.0",
        "myclabs/deep-copy": "^1.5",
        "sebastian/comparator": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "phpspec/phpspec": "^2.1",
        "fzaninotto/faker": "1.4.*"
    },
    "autoload": {
        "psr-0": {
            "Isolate\\UnitOfWork": "src/",
            "Isolate\\UnitOfWork\\Tests": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
