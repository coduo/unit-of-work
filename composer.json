{
    "name": "isolate/unit-of-work",
    "description": "Generic unit of work",
    "require": {
        "symfony/property-access": "~2.1",
        "symfony/event-dispatcher": "~2.1",
        "myclabs/deep-copy": "1.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "phpspec/phpspec": "2.1.*",
        "fzaninotto/faker": "1.4.*"
    },
    "autoload": {
        "psr-0": {
            "Isolate\\UnitOfWork": "src/",
            "Isolate\\UnitOfWork\\Tests": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT",
